{
    "name": "arandilopez/laravel-feed-parser",
    "description": "Laravel and Lumen rss feed parser",
    "require": {
        "php": ">=5.5.9",
        "simplepie/simplepie": "^1.3",
        "illuminate/support": "^5.1",
        "illuminate/contracts": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*",
        "mockery/mockery": "^0.9.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Arandi Lopez",
            "email": "arandilopez.93@gmail.com"
        }
    ],
    "autoload": {
      "psr-4": {
        "ArandiLopez\\Feed\\": "src/"
      }
    },
    "autoload-dev": {
      "classmap": [
        "tests/"
      ]
    },
    "minimum-stability": "stable",
    "suggest": {
        "vlucas/phpdotenv": "Required to use .env files (~1.0)."
    }
}
